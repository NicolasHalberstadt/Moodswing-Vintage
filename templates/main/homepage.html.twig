{% extends 'base.html.twig' %}

{% block title %}Moodswing Vintage{% endblock %}

{% block body %}

<div>
    <h1>Bienvenue sur Moodswing Vintage ✅</h1>
</div>
<div>
{% for product in products %}
<h2>{{ product.name }}</h2>
<p>{{ product.description }}</p>
<p><strong>{{ product.price }} €</strong></p>
<p>Taile du produit : {{ product.size }}</p>
<p>Catégorie : {{ product.category.name }}</p>
{% endfor %}



{% endblock %}
