			<a href="{{ path('homepage') }}">home</a>
		</nav>
		<nav class="home-socialsbtn">
			<a href="https://www.instagram.com/moodswingvintage/" title='Moodswing Vintage on Instagram' target='_blank'>
				<i class="fab fa-instagram"></i>
			</a>
			<a href="{{ path('contact') }}" class="home-socialbtn-email" title="Contact form">&#x40;</a>

			{% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
				
